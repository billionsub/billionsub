{
    "name": "theqdev/justfans",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-shield": "^4.0.2",
        "devfactory/minify": "^2.0.2",
        "filament/filament": "~4.0",
        "filament/spatie-laravel-settings-plugin": "^v4.1.1",
        "firebase/php-jwt": "^6.11",
        "flowframe/laravel-trend": "^v0.4.0",
        "graham-campbell/markdown": "^v16.0.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7.2",
        "jenssegers/agent": "^2.6",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^v12.32.5",
        "laravel/socialite": "^v5.17.0",
        "laravel/tinker": "^v2.10.0",
        "laravel/ui": "^v4.6.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^v3.6.4",
        "mercadopago/dx-php": "^2.6",
        "mews/purifier": "^3.4.2",
        "opencoconut/coconut": "^3.0.3",
        "pbmedia/laravel-ffmpeg": "^8.7.1",
        "pion/laravel-chunk-upload": "^v1.5.6",
        "pusher/pusher-php-server": "^7.2.7",
        "pxlrbt/filament-excel": "^v3.1.0",
        "rahul900day/laravel-captcha": "^4.0",
        "ramsey/uuid": "^4.9.1",
        "parfaitementweb/filament-password-input": "^v4.0.0",
        "saade/filament-laravel-log": "^v4.0.0",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/laravel-permission": "^6.17",
        "spatie/schema-org": "^3.23.0",
        "stripe/stripe-php": "^7.77",
        "symfony/http-client": "^6",
        "symfony/mailgun-mailer": "^6.0",
        "symfony/yaml": "^6.4",
        "verotel/flexpay-php-client": "^5.1",
        "yabacon/paystack-php": "^2.2",
        "zanysoft/laravel-zip": "^3.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.14.10",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "~4.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^v8.8.2",
        "orangehill/iseed": "^3.0",
        "phpmd/phpmd": "^2.15",
        "phpunit/phpunit": "^11.5.42",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.2.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeders",
            "database/factories",
            "app/Model"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
